{
  "name": "RES Continuous Music",
  "version": "1.0",
  "manifest_version" : 2,
  "description": "Allows you to continuously play music on a music subreddit.",
  "browser_action": {
      "default_popup" : "player.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.reddit.com/*",
        "https://www.reddit.com/*"
      ],
      "js": [
        "jquery-3.0.0.min.js",
        "background.js"
      ]
    }
  ]
}
